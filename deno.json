{
  "imports": {
    "cliffy/ansi/colors": "https://deno.land/x/cliffy@v0.25.7/ansi/colors.ts",
    "cliffy/command": "https://deno.land/x/cliffy@v0.25.7/command/mod.ts",
    "cliffy/table": "https://deno.land/x/cliffy@v0.25.7/table/mod.ts",
    "kia": "https://deno.land/x/kia@0.4.1b/mod.ts",
    "std/": "https://deno.land/std@0.199.0/"
  },
  "fmt": {
    "exclude": ["target", "lib"]
  },
  "lint": {
    "exclude": ["target", "lib"]
  },
  "tasks": {
    "build": "deno run -A https://deno.land/x/wasmbuild@0.15.0/main.ts",
    "cli": "deno run --allow-read --allow-net --allow-hrtime cli.ts",
    "test": "deno task test:rust && deno task test:deno",
    "test:deno": "deno test --allow-read=..",
    "test:rust": "cargo test"
  }
}
